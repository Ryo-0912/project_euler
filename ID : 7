line = []
(2..300000).each do |sorg|
  sqrt = Math.sqrt(sorg)
  fact = (2..sqrt).any? {|i| sorg % i == 0 }
  line.push(sorg) unless fact
end
puts line[10000]

# 追記
# こんな書き方もあるみたい。でも、n-1まで調べてる。。
number = *1..100
def is_prime?(n)
    return false if n ==1
    (2..n-1).map{|i| return false if n%i==0}
end
number.map{|n| puts n if is_prime?(n)}
